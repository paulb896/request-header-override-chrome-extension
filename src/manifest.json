{
  "manifest_version": 3,
  "name": "Request Header Override Manifest Version 3",
  "version": "0.0.2",
  "description": "A chrome extension for overriding external http request headers",
  "options_page": "options.html",
  "action": {
    "icon": "main-icon-38.png",
    "default_popup": "popup.html",
    "default_icon": {
      "38": "main-icon-38.png"
    },
    "default_title": "Modify Outgoing XHR/Fetch Request Headers"
  },
  "icons": {
    "128": "main-icon-128.png"
  },
  "permissions": [
    "storage",
    "declarativeNetRequest",
    "declarativeNetRequestFeedback"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "devtools_page": "devtools.html"
}